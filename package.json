{
  "name": "maleci-gpu-task-runner",
  "version": "0.1.0",
  "description": "Allows to queue gpu tasks and run them when resources are free",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "install-gui": "cd gui && npm install && cd ..",
    "start-gui": "cd gui && npm start",
    "start": "cd gui && npm run build && cd .. && sudo \"$(which node)\" server.js",
    "executor": "sudo \"$(which node)\" executor.js",
    "test": "sudo \"$(which node)\" ./runAllTests.js"
  },
  "keywords": [
    "gpu",
    "task runner",
    "maleci"
  ],
  "author": "Nikita Lazarenko <lazarenkonikita1993@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "open": "^10.1.0",
    "serve-static": "^1.16.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^20.3.1"
  }
}
